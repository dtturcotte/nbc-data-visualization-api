		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"google-auth-library": "^9.3.0",
		"googleapis": "^128.0.0",
		"knex": "^3.0.1",
		"morgan": "^1.10.0",
		"pako": "^2.1.0",
